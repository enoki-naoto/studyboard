<div class="wrapper-stanard text-center">
  <div class="container container-standard">
    <div class="row">
      <h1 class="text-white margin40">学習カテゴリー　一覧</h1>
      <div class="col-md-offset-3 col-md-6 col-md-offset-3">
          <% @categorys.each do|category| %>
            <% if logged_in? && current_user.administrator? %>
              <%= link_to '編集', edit_category_path(category) %>
              <%= link_to '削除', category_path(category), method: :delete %>
              <%= link_to category.name,category_questions_path(category),class: 'btn btn-block btn-white' %>
            <% else %>
              <%= link_to category.name,category_questions_path(category),class: 'btn btn-block btn-white' %>
            <% end %>
          <% end %>
      </div>
    </div>
  </div>
</div>