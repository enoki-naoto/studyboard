<div class="top-question-index-wrapper text-left">
  <div class="container question-index-container">
    <div class="row">
      <div class="col-md-offset-3 col-md-6 text-white text-center categoryName">
        <h1>質問一覧</h1>
      </div>
      <div class="col-md-offset-3 col-md-6">
            <% @category.questions.each do|question| %>
              <%= link_to question.title,category_question_path(@category,question),class: 'btn btn-block btn-white' %>
            <% end %>
      <%= link_to '質問を投稿する',new_category_question_path(category_id: @category.id),class:'text-white text-left' %>
     </div>
  </div>
</div>
